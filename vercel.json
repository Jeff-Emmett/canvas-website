{
  "buildCommand": "yarn build",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/posts/(.*)",
      "destination": "/"
    },
    {
      "source": "/board/(.*)",
      "destination": "/"
    },
    {
      "source": "/inbox",
      "destination": "/"
    },
    {
      "source": "/books",
      "destination": "/"
    }
  ]
}