{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/board/(.*)",
      "destination": "/"
    },
    {
      "source": "/board",
      "destination": "/"
    },
    {
      "source": "/inbox",
      "destination": "/"
    },
    {
      "source": "/dashboard",
      "destination": "/"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}